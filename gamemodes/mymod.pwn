	    {
	        SendClientMessage(playerid,COLOR_WHITE, "Admin olarak grevde deilsin.");
			return 1;
	    }

		if(EventInfo[EventStatus] == 3) {
			EventInfo[EventStatus] = 4;

			foreach(Player, i) {
				if(GetPVarInt(i, "EventToken") == 1) {
					if(EventInfo[EventType] == 1) {

			}
			JobDuty[playerid] = 0;
			if(PlayerInfo[playerid][pJob] == 7) {
				Mechanics -= 1;
			} else if(PlayerInfo[playerid][pJob] == 2) {
				Lawyers -= 1;
			}
			DestroyProgressBar(LoadTruckBar[playerid]);

		}
		if(JobDuty[playerid] != 0) return SendClientMessage(playerid,COLOR_GREY,"Avukat yada Tamirci Meslekleinden kmanz gerekiyor.");
		new newcar = GetPlayerVehicleID(playerid);
		if(IsAnTaxi(newcar))

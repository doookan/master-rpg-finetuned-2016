				}
				format(string, sizeof(string), "* %s adl kiiye %d gram potu %d$ karl satmay nerdin.", GetPlayerNameEx(giveplayerid), needed, money);
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);

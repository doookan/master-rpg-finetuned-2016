                }
                else
				{
                    SendClientMessage(playerid, COLOR_GREY, "Arabana ya da oyuncuya yeterince yakn deilsin!");
                    return 1;
                }
            }
        }
        else
		{
            SendClientMessage(playerid, COLOR_GREY, "Geersiz oyuncu.");
            return 1;
        }
    }
    else
	{
        SendClientMessage(playerid, COLOR_GRAD2, "   LSFMD yesi deilsin!");
    }
    return 1;
}


                        GetVehiclePos(carid, pos[0], pos[1], pos[2]);
                        if(floatcmp(floatabs(floatsub(pos[0], pos[3])), 10.0) != -1 &&
                            floatcmp(floatabs(floatsub(pos[1], pos[4])), 10.0) != -1 &&

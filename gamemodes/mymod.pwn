						format(string, sizeof(string), "* %s adl oyuncuyu '%s''a katlmas iin davet ettin.",GetPlayerNameEx(giveplayerid), FamilyInfo[family][FamilyName]);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);

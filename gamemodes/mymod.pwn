CMD:iyilestir(playerid, params[])
{
	if(GetPVarInt(playerid, "EventToken") == 1 && EventInfo[EventToggleHeal] == 1)
		return SendClientMessage(playerid, COLOR_GRAD2, "Etkinlikte iyiletiremezsin!");

	new

		return SendClientMessage(playerid, COLOR_GRAD2, "Gen padawan, Silah yasaklamasndan dolay 2 saat oynamadan nce etkinlie katlamazsn.");

	if(EventInfo[EventPlayers] >= EventInfo[EventLimit] && EventInfo[EventTime] == 0)

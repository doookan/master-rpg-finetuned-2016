		GetPlayerIp(playerid, ip, sizeof(ip));
		format(string,sizeof(string),"%s (IP: %s) $%d paray %d. ailenin kasasna koydu.",GetPlayerNameEx(playerid),ip,amount,PlayerInfo[playerid][pGang]);

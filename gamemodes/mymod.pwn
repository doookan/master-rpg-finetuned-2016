					else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 599) AttachDynamicObjectToVehicle(GetPVarInt(playerid, "SirenO"), GetPlayerVehicleID(playerid), 0.000000, 0.039999, 1.153999, 0.000000, 0.000000, 0.000000);
